from typing import Optional, Union

from . import event

def get_ticks() -> int: ...
def wait(milliseconds: int) -> int: ...
def delay(milliseconds: int) -> int: ...
def set_timer(event_type: Union[int, event.Event], milliseconds: int, once: Optional[bool] = ...) -> None: ...

class Clock:
    def tick(self, framerate: Optional[int] = ...) -> int: ...
    def tick_busy_loop(self, framerate: Optional[int] = ...) -> int: ...
    def get_time(self) -> int: ...
    def get_rawtime(self) -> int: ...
    def get_fps(self) -> float: ...

